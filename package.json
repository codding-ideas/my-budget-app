{
  "name": "ibudget-app",
  "version": "1.0.0",
  "description": "This app helps you to manage your your resources which uses modern technologies: React-Redux, Node JS, Express Js, Mongodb-Mongoose",
  "main": "app.js",
  "engines": {
    "node": "v10.15.1",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node app.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "debug": "ndb app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tweneboah/iBudget-app.git"
  },
  "keywords": [
    "Income",
    "Expenses"
  ],
  "author": "Emmanuel Tweneboah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tweneboah/iBudget-app/issues"
  },
  "homepage": "https://github.com/tweneboah/iBudget-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.6.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
